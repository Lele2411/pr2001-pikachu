<% if @category.errors.blank? %>
	$('#categories').append('<%= j(render('admin/categories/category', category: @category))%>');
	$('#category_messages').prepend('<%= j(render('admin/images/success',a: "A Category Created!")) %>');
<% else %>
	$('#category_messages').prepend('<%= j(render('shared/error_messages', object: @category)) %>');
<%end%>